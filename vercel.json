{
  "version": 2,
  "builds": [
    {
      "src": "backend/json-based/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/diagnose",
      "destination": "backend/json-based/app.py"
    },
    {
      "source": "/api/symptoms",
      "destination": "backend/json-based/app.py"
    },
    {
      "source": "/api/health",
      "destination": "backend/json-based/app.py"
    }
  ]
}
